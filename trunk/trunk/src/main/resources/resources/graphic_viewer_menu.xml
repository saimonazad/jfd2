<?xml version="1.0" encoding="UTF-8"?>
<menubar>
	<menuitem label="exit" command="close">
		<key name="VK_ESCAPE"/>
	</menuitem>
	<menuitem label="full_screen" command="full_screen">
		<key name="VK_A"/>
	</menuitem>
	<separator/>
	<menuitem label="zoom_out" command="zoom_out">
		<key name="VK_CLOSE_BRACKET"/>
	</menuitem>
	<menuitem label="zoom_in" command="zoom_in">
		<key name="VK_OPEN_BRACKET"/>
	</menuitem>
	<menuitem label="rotate_left" command="rotate_left">
		<key name="VK_SEMICOLON"/>
	</menuitem>
	<menuitem label="rotate_right" command="rotate_right">
		<key name="VK_COLON"/>
	</menuitem>
	<separator/>
	<menuitem label="slideshow" command="slideshow">
		<key name="VK_S"/>
	</menuitem>
</menubar>
